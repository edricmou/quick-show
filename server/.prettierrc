{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": [
        "*.js",
        "*.ts",
        "*.mjs",
        "*.cjs"
      ],
      "options": {
        "trailingComma": "es5"
      }
    },
    {
      "files": [
        "*.json",
        ".eslintrc",
        ".prettierrc",
        "package.json"
      ],
      "options": {
        "printWidth": 200,
        "tabWidth": 2
      }
    },
    {
      "files": [
        "*.yml",
        "*.yaml"
      ],
      "options": {
        "tabWidth": 2,
        "singleQuote": false
      }
    },
    {
      "files": [
        "*.sh"
      ],
      "options": {
        "endOfLine": "lf",
        "tabWidth": 2
      }
    }
  ]
}